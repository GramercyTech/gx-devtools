    "@fal-works/esbuild-plugin-global-externals": "^2.1.2",
    "@gramercytech/gx-componentkit": "^1.0.8",
    "@vitejs/plugin-vue": "^5.1.4",
    "chrome-launcher": "^1.1.2",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "mkcert": "^3.2.0",
    "nodemon": "^3.1.7",
    "rollup-plugin-external-globals": "^0.12.0",
    "shelljs": "^0.8.5",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0",
    "vite": "^5.4.8",
    "vue": "^3.5.8",
    "web-ext": "^8.2.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "mkcert": "^3.2.0",
    "nodemon": "^3.1.7"
  }
