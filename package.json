{
  "name": "@gramercytech/gx-toolkit",
  "version": "1.0.104",
  "description": "Dev tools for create platform plugins",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently 'vite dev' 'nodemon server.js'",
    "dev-app": "vite dev",
    "ext:firefox": "web-ext run --source-dir browser-extensions/firefox",
    "ext:firefox-build": "web-ext build --source-dir browser-extensions/firefox --artifacts-dir dist/firefox",
    "ext:chrome": "node scripts/launch-chrome.js",
    "ext:chrome-pack": "node scripts/pack-chrome.js",
    "ext:install": "npm run ext:firefox & npm run ext:chrome",
    "ext:build": "npm run ext:firefox-build && npm run ext:chrome-pack"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/GramercyTech/gx-toolkit.git"
  },
  "bin": {
    "gxto": "bin/gx-toolkit.js"
  },
  "keywords": [
    "tunnel",
    "gtcp",
    "eventfinity",
    "gxp"
  ],
  "author": "Peek",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GramercyTech/gx-toolkit/issues"
  },
  "homepage": "https://github.com/GramercyTech/gx-toolkit#readme",
